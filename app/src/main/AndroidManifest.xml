<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swifta.mats"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".service.BackgroundServices"
            android:icon="@drawable/ic_launcher"
            android:label="@string/service_name" />

        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" />
        <activity
            android:name=".WithdrawalActivity"
            android:label="@string/title_activity_withdrawal"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.HomeActivity" />
        </activity>
        <activity
            android:name=".MMOperatorsActivity"
            android:label="@string/title_activity_mmoperators"
            android:parentActivityName=".WithdrawalActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.WithdrawalActivity" />
        </activity>
        <activity
            android:name=".forms.WithdrawMMActivity"
            android:label="@string/title_activity_withdraw_mm"
            android:parentActivityName=".MMOperatorsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.MMOperatorsActivity" />
        </activity>
        <activity
            android:name=".FloatTransferActivity"
            android:label="@string/title_activity_float_transfer"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.HomeActivity" />
        </activity>
        <activity
            android:name=".forms.DepositFloatActivity"
            android:label="@string/title_activity_deposit_float"
            android:parentActivityName=".FloatTransferActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.FloatTransferActivity" />
        </activity>
        <activity
            android:name=".forms.CompleteDepositFloatActivity"
            android:label="@string/title_activity_complete_deposit_float"
            android:parentActivityName=".FloatTransferActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.swifta.mats.FloatTransferActivity" />
        </activity>
        <activity
            android:name=".AccountActivity"
            android:label="@string/title_activity_account" />
        <activity
            android:name=".MiniStatementActivity"
            android:label="@string/title_activity_mini_statement" />
        <activity android:name=".forms.CashInActivity" />
        <activity android:name=".forms.CompleteCashOutActivity" />
        <activity android:name=".forms.CashOutUnregisteredCustomerActivity"></activity>
    </application>

</manifest>
